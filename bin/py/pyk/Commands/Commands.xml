<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE Commands>
<Commands>
    <command>
        <!-- LaTeX -->
        <cmd context=".tex" key="F1" args="-interaction=nonstopmode %baseName.tex" name="pdfLaTeX" icon="" wd="">pdflatex</cmd>
        <cmd context=".tex" key="F2" args="%baseName.pdf" name="view PDF" icon="" wd="">evince</cmd>

        <!-- Python -->
        <cmd context=".py" key="F1" args="%baseName.py" name="launch Python" icon="" wd="">python</cmd>

        <!-- Lout -->
        <cmd context=".lt" key="F1" args="%baseName.lt -o %baseName.ps" name="Lout Compile" icon="" wd="">lout</cmd>
        <cmd context=".lt" key="F2" args="%baseName.ps" name="Lout view PS" icon="" wd="">evince</cmd>
        <cmd context=".lt" key="F3" args="-C%home/myfonts -D%home/myfonts -F%home/myfonts %baseName.lt -o %baseName.ps" name="Lout Compile with fonts" icon="" wd="">lout</cmd>

        <!-- Asymptote -->
        <cmd context=".asy" key="F1" args="%baseName.asy -V -d -batchView -o %baseName.pdf" name="Compile and View PDF" icon="" wd="">asy</cmd>
        <cmd context=".asy" key="F2" args="%baseName.asy -noV -wait -o %baseName.pdf" name="PDF compile" icon="" wd="">asy</cmd>
        <cmd context=".asy" key="F3" args="%baseName.pdf" name="view PDF" icon="" wd="">xpdf</cmd>
        <cmd context=".asy" key="F4" args="-geometry 700 -density 350 -units PixelsPerInch %baseName.pdf %baseName.png" name="Generate a PNG from PDF" icon="" wd="">convert</cmd>
        <cmd context=".asy" key="F5" args="%baseName.png" name="View PNG" icon="" wd="">eog</cmd>

        <!-- reStructuredText -->
        <cmd context=".rst" key="F1" args="to_html.py --stylesheet-path=default.css %realName.rst %realName.html" name="reStructuredText process HTML" icon="" wd="%sciptsDir">python</cmd>
        <cmd context=".rst" key="F2" args="-new-window %baseName.html" name="reStructuredText view HTML" icon="" wd="">firefox</cmd>
        <cmd context=".rst" key="F3" args="to_tex.py --stylesheet-path=default.sty %realName.rst %realName.tex" name="reStructuredText process LaTeX" icon="" wd="%sciptsDir">python</cmd>
        <cmd context=".rst" key="F4" args="to_odt.py -g --add-syntax-highlight --stylesheet-path=default.odt %realName.rst %realName.odt" name="reStructuredText process OpenOffice" icon="" wd="%sciptsDir">python</cmd>
        <cmd context=".rst" key="F5" args="to_lout.py %realName.rst %realName.lt" name="reStructuredText to Lout" icon="" wd="%sciptsDir">python</cmd>

        <!-- You need to provide HOST NAME PASSWORD REMOTEDIR in there -->
        <cmd context=".rst" key="F6" args="transfer_to_ftp.py %realName.html HOST NAME PASSWORD REMOTEDIR" name="FTP Transfert my file" icon="" wd="%sciptsDir">python</cmd>
    </command>
</Commands>

