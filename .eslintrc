{
"parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "jquery": true,
    "shelljs": true,
    "mongo": true,
    "meteor": true,
    "es6": true
  },
  "settings": {
  "ecmascript": 6,
  "jsx": true
  },
  "plugins": [ "react" ],
  "rules": {
    "strict": 0,
    "quotes": 0,
    "no-unused-vars": 0,
    "camelcase": 0,
    "no-underscore-dangle": 0
  },
  "ecmaFeatures": {
    "jsx": true,
    "forOf": true,
    "modules": true,
    "arrowFunctions": true,
    "binaryLiterals": true,
    "blockBindings": true,
    "generators": true
  }
}
